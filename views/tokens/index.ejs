<!DOCTYPE html>
<html lang="en">
<head>
    <% include ../partials/head %>
    <title>Tokens</title>
</head>
<body>
    <% include ../partials/header %>
    <main>
        <h2>Tokens</h2>
    
        <% tokens.forEach(token => { %>
            <a class="row" href="/tokens?id=<%= token.id %>">
                <div class="col-sm-4">
                    <%= token.id %>
                </div>
                <div class="col-sm-4">
                    <%= token.name %>
                </div>
                <div class="col-sm-4">
                    <%= token.ticker %>
                </div>
            </a>

        <% });%>


    </main>
    <% include ../partials/footer %>
</body>
</html>